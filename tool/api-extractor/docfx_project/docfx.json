{
	"metadata": [
		{
			"src": [
				{
					"files": ["src/**.csproj"]
				}
			],
			"dest": "api",
			"includePrivateMembers": false,
			"disableGitFeatures": false,
			"disableDefaultFilter": false,
			"noRestore": false,
			"namespaceLayout": "flattened",
			"memberLayout": "samePage",
			"allowCompilationErrors": false
		}
	],
	"build": {
		"content": [
			{
				"files": ["api/**.yml", "api/index.md"]
			},
			{
				"files": ["articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md", "api/*.md"]
			}
		],
		"resource": [
			{
				"files": ["images/**"]
			}
		],
		"overwrite": [
			{
				"files": ["apidoc/**.md"],
				"exclude": ["obj/**", "_site/**"]
			}
		],
		"dest": "_site",
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": ["default", "templates/singulinkfx/singulinkfx"],
		"postProcessors": [],
		"keepFileLink": false,
		"disableGitFeatures": false
	}
}
